[
  {
    "kernelSource": {
      "name": "Xiaomi_10_lite_5G",
      "repo": "https://github.com/mikairyuu/kernel_xiaomi_sm7250.git",
      "branch": "神速",
      "device": "monet",
      "defconfig": "monet_defconfig"
    },
    "toolchains": [
      {
        "repo": "https://gitlab.com/LeCmnGend/proton-clang.git",
        "branch": "clang-17",
        "name": "clang",
        "binaryEnv": [
          "bin"
        ]
      },
      {
        "repo": "https://github.com/ZyCromerZ/aarch64-zyc-linux-gnu.git",
        "brach": "14",
        "name": "gcc64",
        "binaryEnv": [
          "bin"
        ]
      },
       {
        "repo": "https://github.com/ZyCromerZ/arm-zyc-linux-gnueabi.git",
        "brach": "14",
        "name": "gcc",
        "binaryEnv": [
          "bin"
        ]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
        "CROSS_COMPILE": "aarch64-zyc-linux-gnu-",
        "CROSS_COMPILE_ARM32": "arm-zyc-linux-gnueabi-",
        "LD": "ld.lld",
        "AR": "llvm-ar",
        "NM": "llvm-nm",
        "OBJCOPY": "llvm-objcopy",
        "OBJDUMP": "llvm-objdump",
        "READELF": "llvm-readelf",
        "OBJSIZE": "llvm-size",
        "STRIP": "llvm-strip",
        "LDGOLD": "aarch64-zyc-linux-gnu-ld.gold",
        "LLVM_AR": "llvm-ar",
        "LLVM_DIS": "llvm-dis",
        "CONFIG_THINLTO": ""
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": false
    },
    "enableKernelSU": true,
    "enableLXC": false
  }
]
