[
  {
    "kernelSource": {
      "name": "Xiaomi10Lite5G",
      "repo": "https://github.com/mikairyuu/kernel_xiaomi_sm7250.git",
      "branch": "神速",
      "device": "monet",
      "defconfig": "monet_defconfig"
    },
    "toolchains": [
      {
        "name": "clang",
        "url": "https://android.googlesource.com/platform/prebuilts/clang/host/linux-x86/+archive/refs/heads/main/clang-r510928.tar.gz",
        "binaryEnv": [
          "bin"
        ]
      },
      {
        "name": "gcc64",
        "repo": "https://github.com/LineageOS/android_prebuilts_gcc_linux-x86_aarch64_aarch64-linux-android-4.9.git",
        "branch": "lineage-19.1",
        "binaryEnv": [
          "bin"
        ]
      },
      {
        "name": "gcc",
        "repo": "https://github.com/LineageOS/android_prebuilts_gcc_linux-x86_arm_arm-linux-androideabi-4.9.git",
        "branch": "lineage-19.1",
        "binaryEnv": [
          "bin"
        ]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
        "CLANG_COMPILE": "aarch64-linux-gnu-",
        "CROSS_COMPILE": "gcc64/bin/aarch64-linux-android-",
        "CROSS_COMPILE_COMPAT": "gcc/bin/arm-linux-androideabi-",
        "LD": "ld.lld",
        "AR": "llvm-ar",
        "NM": "llvm-nm",
        "OBJCOPY": "llvm-objcopy",
        "OBJDUMP": "llvm-objdump",
        "READELF": "llvm-readelf",
        "OBJSIZE": "llvm-size",
        "LDGOLD": "aarch64-linux-android-ld.gold"
        "STRIP": "llvm-strip",
        "LLVM_AR": "llvm-ar",
        "LLVM_DIS": "llvm-dis"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": false,
      "custom": {
        "repo": "https://github.com/sibal391/AnyKernel3.git",
        "branch": "monet"
      }
    },
    "enableKernelSU": true,
    "enableLXC": false
  }
]
